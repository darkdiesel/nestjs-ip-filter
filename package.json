{
  "name": "nestjs-ip-filter",
  "version": "0.3.0",
  "description": "A NestJS Guard for filtering IP addresses",
  "author": "Deokgyu Yang <secugyu@gmail.com>",
  "keywords": [
    "nestjs",
    "ip filter",
    "whitelist",
    "blacklist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/awesometic/nestjs-ip-filter.git"
  },
  "homepage": "https://github.com/awesometic/nestjs-ip-filter",
  "bugs": {
    "url": "https://github.com/awesometic/nestjs-ip-filter/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"{lib,example}/**/*.ts\"",
    "prepublish:npm": "npm run lint && npm run format && npm run build",
    "publish:npm": "npm publish --access public",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{lib,apps,libs,test,example/src}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@supercharge/request-ip": "^1.2.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
        "eslint": "^9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "30.0.5",
    "prettier": "3.6.2",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
